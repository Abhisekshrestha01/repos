@model IEnumerable<MvcDepartmentEmployee.Models.Employee>

@{
    ViewBag.Title = "Employee List";
}
@using MvcDepartmentEmployee.Models;
<h2>Employee List</h2>
<ul>
    @foreach(Employee emp in @Model)
    {
        <li>
            @Html.ActionLink(emp.Name, "Details", new { id = emp.EmployeeId }, null)           
        </li>
    }
</ul>
@Html.ActionLink("Back to DepList", "Index", "Department")
